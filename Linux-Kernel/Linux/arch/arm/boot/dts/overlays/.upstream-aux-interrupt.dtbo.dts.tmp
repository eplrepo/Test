# 1 "arch/arm/boot/dts/overlays/upstream-aux-interrupt-overlay.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm/boot/dts/overlays/upstream-aux-interrupt-overlay.dts"


/dts-v1/;
/plugin/;

/ {
    compatible = "brcm,bcm2708";

    fragment@0 {
        target = <&uart1>;
        __overlay__ {
            interrupt-parent = <&intc>;
            interrupts = <0x1 0x1d>;
        };
    };

    fragment@1 {
        target = <&spi1>;
        __overlay__ {
            interrupt-parent = <&intc>;
            interrupts = <0x1 0x1d>;
        };
    };

    fragment@2 {
        target = <&spi2>;
        __overlay__ {
            interrupt-parent = <&intc>;
            interrupts = <0x1 0x1d>;
        };
    };
};
